<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- TemplateTool Specific -->
	<SolidColorBrush x:Key="Window2BackgroundBrush" Color="#FF3B3B3B" />
	<Style x:Key="Window2Style" TargetType="{x:Type Window}">
		<Setter Property="FontFamily" Value="Ubuntu" />
		<Setter Property="FontSize" Value="10pt" />
		<Setter Property="SnapsToDevicePixels" Value="True" />
		<Setter Property="UseLayoutRounding" Value="True" />
		<Setter Property="Background" Value="#FF1C1C1C" />
		<Setter Property="Foreground" Value="#FF7C7C7C" />
	</Style>
</ResourceDictionary>