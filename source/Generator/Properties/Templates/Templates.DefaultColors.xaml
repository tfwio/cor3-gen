<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
	<!--   -->
	<Color x:Key="ColorHeaderLight">White</Color>
	<Color x:Key="ColorHeaderMid">#7FD2D2D2</Color>
	<Color x:Key="ColorHeaderDark">#7FA1A1A1</Color>
	<Color x:Key="ColorHeaderDarkAlt">#7F7AC489</Color>
	<Color x:Key="ColorHeaderLightInvalid">#FFF7FF79</Color>
	<Color x:Key="ColorHeaderDarkInvalid">#FF20FF51</Color>
	<Color x:Key="color_HeaderFocused_Light">White</Color>
	<Color x:Key="color_HeaderFocused_Middle">#FFD1D1D1</Color>
	<!--   -->
	
	
	<!-- Generic brushes -->
	<SolidColorBrush x:Key="DefaultControlBorderBrush" Color="#FF688CAF"/>
	<SolidColorBrush x:Key="DefaultControlBackgroundBrush" Color="#FFE3F1FE"/>
	<SolidColorBrush x:Key="DefaultControlForegroundBrush" Color="#FF10257F"/>
	
	
	<!--   -->
	<LinearGradientBrush x:Key="AlternateRowBackgroundBrush" StartPoint="0.5,0.0" EndPoint="0.5,1.0">
		<GradientStop Color="#FFFEFEFF" Offset="0"/>
		<GradientStop Color="#FFE4F0FC" Offset="1"/>
	</LinearGradientBrush>
	
	
	
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>